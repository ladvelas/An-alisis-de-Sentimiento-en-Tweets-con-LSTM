    "import re\n",
    "\n",
    "def clean_text(text):\n",
    "    text = re.sub(r'@\\w+|#\\w+|http\\S+', '', text)  # menciones, hashtags, urls\n",
    "    text = re.sub(r'[^A-Za-z0-9\\s]', '', text)  # s√≠mbolos\n",
    "    return text.lower()\n"
   ]
